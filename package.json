{
  "name": "devkit-cli",
  "version": "1.0.3",
  "description": "Professional fullstack project generator CLI (FastAPI + React, Express + Vue, multi-db, Docker/Podman ready)",
  "main": "dist/index.js",
  "bin": {
    "devkit": "dist/index.js"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "cli",
    "generator",
    "fullstack",
    "fastapi",
    "react",
    "express",
    "vue",
    "docker",
    "podman"
  ],
  "author": "juanvidev1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/juanvidev1/devkit-cli.git"
  },
  "files": [
    "dist/",
    "templates/",
    "README.md",
    "README.es.md"
  ],
  "dependencies": {
    "commander": "^14.0.1",
    "ejs": "^3.1.10",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.2",
    "inquirer": "^12.9.6",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.6.2",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
